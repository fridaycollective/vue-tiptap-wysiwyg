{
  "name": "vue-tiptap-wysiwyg",
  "version": "0.1.24",
  "description": "Vue WYSIWYG Editor based on tiptap",
  "author": "Dean Hopkins",
  "main": "dist/vue-tiptap-wysiwyg.umd.min.js",
  "jsnext:main": "dist/vue-tiptap-wysiwyg.umd.min.js",
  "private": false,
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "components:parse": "node ./docs/.scripts/parse.js",
    "components:watch": "watch \"npm run components:parse\" ./src",
    "lib": "vue-cli-service build --target lib --name vue-tiptap-wysiwyg ./src/index.js",
    "docs:dev": "concurrently \"npm run components:watch\" \"vuepress dev docs\"",
    "docs:build": "vuepress build docs",
    "prepublishOnly": "npm run lib",
    "unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@tiptap/extension-color": "^2.0.0-beta.3",
    "@tiptap/extension-gapcursor": "^2.0.0-beta.25",
    "@tiptap/extension-highlight": "^2.0.0-beta.22",
    "@tiptap/extension-image": "^2.0.0-beta.16",
    "@tiptap/extension-link": "^2.0.0-beta.21",
    "@tiptap/extension-placeholder": "^2.0.0-beta.32",
    "@tiptap/extension-task-item": "^2.0.0-beta.19",
    "@tiptap/extension-task-list": "^2.0.0-beta.17",
    "@tiptap/extension-text-align": "^2.0.0-beta.23",
    "@tiptap/extension-text-style": "^2.0.0-beta.17",
    "@tiptap/extension-typography": "^2.0.0-beta.15",
    "@tiptap/starter-kit": "^2.0.0-beta.118",
    "@tiptap/vue-2": "^2.0.0-beta.58",
    "vue": "^2.6.11",
    "vue-swatches": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-unit-mocha": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/test-utils": "^1.0.0-beta.30",
    "@vuedoc/parser": "^1.4.0",
    "@vuepress/plugin-back-to-top": "^1.2.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "acorn": "^7.1.0",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0",
    "chai": "^4.1.2",
    "concurrently": "^4.1.2",
    "es6-symbol": "^3.1.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "markdown-it-footnote": "^3.0.2",
    "mockdate": "^2.0.5",
    "node-sass": "^4.11.0",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.2",
    "webpack": "^4.41.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
